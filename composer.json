	"type": "wordpress-plugin",
	"scripts": {
		"phpcs": "phpcs",
		"phpcbf": "phpcbf"
	},
	"autoload": {
		"psr-4": {
			"BracketSpace\\Notification\\bbPress\\": "src/classes"
		}
	},
	"require": {
		"typisttech/imposter-plugin": "^0.5.0",
		"micropackage/requirements": "^1.0",
		"micropackage/dochooks": "^1.0",
		"micropackage/filesystem": "^1.1"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"phpcompatibility/php-compatibility": "^9.1",
		"wp-coding-standards/wpcs": "^2.0"
	},
	"extra": {
		"imposter": {
			"namespace": "BracketSpace\\Notification\\bbPress\\Vendor"
		}
	}
